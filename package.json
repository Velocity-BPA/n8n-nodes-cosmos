{
  "name": "n8n-nodes-cosmos",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Cosmos Hub blockchain providing 12 resources and 80+ operations for ATOM transactions, staking, governance, and IBC transfers. Includes real-time WebSocket triggers for blockchain event monitoring.",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "cosmos",
    "cosmos-hub",
    "atom",
    "blockchain",
    "ibc",
    "staking",
    "governance",
    "cosmjs",
    "tendermint",
    "interchain",
    "web3",
    "cryptocurrency",
    "defi"
  ],
  "license": "BUSL-1.1",
  "homepage": "https://velobpa.com",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-cosmos.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CosmosNetwork.credentials.js",
      "dist/credentials/CosmosApi.credentials.js",
      "dist/credentials/IbcRegistry.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Cosmos/Cosmos.node.js",
      "dist/nodes/Cosmos/CosmosTrigger.node.js"
    ]
  },
  "dependencies": {
    "@cosmjs/amino": "^0.32.4",
    "@cosmjs/crypto": "^0.32.4",
    "@cosmjs/encoding": "^0.32.4",
    "@cosmjs/math": "^0.32.4",
    "@cosmjs/proto-signing": "^0.32.4",
    "@cosmjs/stargate": "^0.32.4",
    "@cosmjs/tendermint-rpc": "^0.32.4",
    "axios": "^1.7.2",
    "bech32": "^2.0.0",
    "bip39": "^3.1.0",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.3.0",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
